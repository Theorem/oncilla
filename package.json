{
  "name": "oncilla",
  "version": "5.5.0",
  "description": "Client-side database for real-time and optimistic UI",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production tsc --project tsconfig.build.json",
    "ci": "npm run verify",
    "format": "prettier --write '{src,typings}/**/*.{js,json,md,ts}' '*.{js,json,md,ts}'",
    "format-verify": "prettier --check '{src,typings}/**/*.{js,json,md,ts}' '*.{js,json,md,ts}'",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "prepublishOnly": "npm run verify && npm run build",
    "start": "ava --watch",
    "start-building": "rm -rf dist && NODE_ENV=production tsc --watch",
    "start-workbench-server": "node workbench/runMemory.js",
    "start-workbench-ui": "NODE_ENV=development webpack-dev-server --config workbench/webpack.config.js --history-api-fallback --hot --client-log-level warning --host 127.0.0.1 --port 3089",
    "test": "ava",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run format-verify && npm run lint && npm run test"
  },
  "dependencies": {
    "nanoevents": "^5.1.11",
    "reconnecting-websocket": "^4.4.0",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@theorem/eslint-plugin": "^4.0.0",
    "@types/nanoevents": "^1.0.0",
    "@types/react": "^16.9.35",
    "@types/ws": "^7.4.0",
    "ava": "^3.15.0",
    "eslint": "^6.8.0",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  },
  "browser": "dist/index.js",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=7"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "repository": "git@github.com:Theorem/oncilla.git",
  "sideEffects": false
}
